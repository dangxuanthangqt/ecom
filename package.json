{
  "name": "ecom",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:swagger": "pnpm run build && APP_ENV=development node dist/src/generate-swagger.js",
    "es-check": "eslint --max-warnings=0 .",
    "types-check": "tsc --noEmit",
    "format-check": "prettier --ignore-unknown --check .",
    "format-fix": "prettier --ignore-unknown --write .",
    "lint": "concurrently --timings --names=\"check \" --prefix=\"[{name}]\" --prefix-colors=\"bgGreen.bold\" \"pnpm:*-check\"",
    "start": "nest start",
    "start:dev": "APP_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:dev:create-only": "prisma migrate dev --create-only",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "seed:initial-scripts": "ts-node initial-scripts",
    "seed:initial-scripts:create-permission:dev": "APP_ENV=development ts-node initial-scripts/create-permission",
    "seed:initial-scripts:create-permission:pod": "APP_ENV=production ts-node initial-scripts/create-permission",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.1",
    "@nestjs/core": "11.0.1",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/swagger": "11.1.1",
    "@prisma/client": "6.4.1",
    "@types/ms": "2.1.0",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "4.1.0",
    "dotenv": "16.4.7",
    "google-auth-library": "9.15.1",
    "googleapis": "146.0.0",
    "ms": "2.1.3",
    "nestjs-pino": "4.3.1",
    "nestjs-zod": "4.3.1",
    "otpauth": "9.4.0",
    "pino-http": "10.4.0",
    "pino-pretty": "13.0.0",
    "reflect-metadata": "0.2.2",
    "resend": "4.1.2",
    "rxjs": "7.8.1",
    "uuid": "11.1.0",
    "yaml": "2.7.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.1",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.7",
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.7",
    "@types/supertest": "6.0.2",
    "concurrently": "9.1.2",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.2",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.4.3",
    "prettier": "3.4.2",
    "prisma": "6.4.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.6.5",
  "pnpm": {
    "onlyBuiltDependencies": [
      "bcrypt"
    ]
  }
}
